#include <stdio.h>
void main()
{
    int a;
    count==0;
    printf("enter any number");
    scanf("%d",&a);
    for(i=2;i<a;i++)
    {
        if(a%i==0)
        {
            count++;
        }
    }
    if(count==0)
    {
        printf("It is a prime number");
        scanf("%d",&a)
    }
    else
    {
        print("it is not prime");
    }
}
    
